{
	"private": true,
	"scripts": {
		"start": "node scripts/umi-runner.js dev",
		"build": "node scripts/umi-runner.js build",
		"build:file": "node scripts/umi-runner.js build --for-file",
		"postinstall": "umi generate tmp",
		"prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
		"test": "umi-test",
		"test:coverage": "umi-test --coverage"
