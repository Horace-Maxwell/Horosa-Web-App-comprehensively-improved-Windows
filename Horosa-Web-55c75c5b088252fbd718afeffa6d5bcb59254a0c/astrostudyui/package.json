		"node-forge": "^1.3.1",
		"numeral": "^2.0.6",
		"print-js": "^1.0.63",
		"qrcode.react": "^1.0.0",
		"react": "17.x",
		"react-dom": "17.x",
		"react-quill": "^1.3.5",
		"three": "^0.128.0",
		"umi": "^3.5.41",
		"video.js": "^8.3.0",
